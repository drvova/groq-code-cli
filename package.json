{
	"name": "groq-code-cli",
	"version": "1.0.2",
	"license": "MIT",
	"bin": {
		"groq": "dist/core/cli.js"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"prepare": "npm run build",
		"test": "ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@inkjs/ui": "^2.0.0",
		"@modelcontextprotocol/sdk": "^1.22.0",
		"chalk": "^5.4.1",
		"commander": "^14.0.0",
		"groq-sdk": "^0.27.0",
		"https-proxy-agent": "^7.0.6",
		"ink": "^6.0.1",
		"meow": "^11.0.0",
		"openai": "^6.9.1",
		"react": "^19.1.0",
		"shiki": "^3.15.0",
		"socks-proxy-agent": "^8.0.5",
		"typescript-language-server": "^4.3.3",
		"typescript": "^5.0.3",
		"vscode-langservers-extracted": "^4.10.0",
		"pyright": "^1.1.390",
		"bash-language-server": "^5.4.0",
		"yaml-language-server": "^1.15.0",
		"@vue/language-server": "^2.1.10",
		"svelte-language-server": "^0.17.5",
		"intelephense": "^1.12.8",
		"deno": "^2.5.6",
		"vscode-jsonrpc": "^8.2.1",
		"vscode-languageserver-protocol": "^3.17.5",
		"vscode-languageserver-types": "^3.17.5",
		"vscode-uri": "^3.1.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/glob": "^8.1.0",
		"@types/react": "^19.1.8",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"madge": "^8.0.0",
		"prettier": "^2.8.7",
		"ts-morph": "^27.0.2",
		"ts-node": "^10.9.1",
		"xo": "^1.2.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		],
		"files": [
			"src/**/*.test.ts",
			"!**/*.d.ts"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
